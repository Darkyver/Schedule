<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.collections.FXCollections?>
<?import java.lang.Integer?>

<!--   fx:controller="com.darkyver.javafxview.controllersView.AddPaymentController"     -->
<VBox  maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="147.0" prefWidth="425.0" spacing="10.0" stylesheets="@../css/application.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <Label fx:id="label" maxWidth="1.7976931348623157E308" text="Добавление новой оплаты пользователя">
            <font>
                <Font name="Arial" size="14.0" />
            </font>
        </Label>
        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" VBox.vgrow="ALWAYS">
            <children>
                <ComboBox fx:id="cbAmount" value="1" editable="true" prefWidth="150.0" promptText="Количество занятий" visibleRowCount="5">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <Integer fx:value="1"/>
                            <Integer fx:value="2"/>
                            <Integer fx:value="3"/>
                            <Integer fx:value="4"/>
                            <Integer fx:value="5"/>
                        </FXCollections>
                    </items>
                </ComboBox>
                <DatePicker fx:id="datePicker" promptText="Дата оплаты" />
            </children>
        </HBox>
        <HBox alignment="TOP_RIGHT" prefHeight="33.0" spacing="10.0" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="btnApply" onAction="#btnApply" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="button-green" text="Добавить" HBox.hgrow="ALWAYS" />
                <Button fx:id="btnCancel" onAction="#btnCancel" layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Отмена" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
    </children>
    <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
